    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Images\Developer\My.jpg" />
    <None Remove="Resources\Images\Movies\Asteroid City.jpg" />
    <None Remove="Resources\Images\Movies\Barbie.jpg" />
    <None Remove="Resources\Images\Movies\Guardians of the Galaxy.jpg" />
    <None Remove="Resources\Images\Movies\Rise of the Beasts .jpg" />
    <None Remove="Resources\Images\Movies\Super Mario movie .jpeg" />
