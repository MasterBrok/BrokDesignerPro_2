  <ItemGroup>
    <Resource Include="Resources\Images\Movies\Super Mario movie .jpeg" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Images\Movies\Guardians of the Galaxy.jpg">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Images\Developer\My.jpg" />
    <Resource Include="Resources\Images\Movies\Asteroid City.jpg" />
    <Resource Include="Resources\Images\Movies\Barbie.jpg" />
